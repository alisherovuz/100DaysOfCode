#main
create table staff
(first_name varchar(20),
last_name varchar(20),
staff_id int,
username varchar(20));

create table actor
(first_name varchar(20),
last_name varchar(20),
actor_id int,
username varchar(20));

create table film
(description varchar(20),
title varchar(20),
film_id int,
genre varchar(20));


#queries for staff table
select * from staff where first_name like '%s%';
select * from staff where last_name like '%s%';
select * from staff where first_name like '%s%' limit 5;
select * from staff where last_name like '%s%' limit 6;
select * from staff where username like '%s%';
select * from staff where username like '%s%' limit 5;
select * from staff where staff_id is null;
select * from staff where staff_id is null limit 5;
select * from staff order by username DESC;
select * from staff order by username limit 5;
select * from staff order by staff_id limit 5;
select * from staff order by staff_id DESC;
select * from staff order by last_name limit 5;
select * from staff order by last_name DESC;
select * from staff order by first_name limit 5;
select * from staff order by first_name DESC;


#queries for actor table
select * from actor where first_name like '%s%';
select * from actor where last_name like '%s%';
select * from actor where first_name like '%s%' limit 5;
select * from actor where last_name like '%s%' limit 6;
select * from actor where username like '%s%';
select * from actor where username like '%s%' limit 5;
select * from actor where actor_id is null;
select * from actor where actor_id is null limit 5;
select * from actor order by username DESC;
select * from actor order by username limit 5;
select * from actor order by actor_id limit 5;
select * from actor order by actor_id DESC;
select * from actor order by last_name limit 5;
select * from actor order by last_name DESC;
select * from actor order by first_name limit 5;
select * from actor order by first_name DESC;

#queries for film table
select * from film where genre like '%s%';
select * from film where title like '%s%';
select * from film where genre like '%s%' limit 5;
select * from film where title like '%s%' limit 6;
select * from film where description like '%s%';
select * from film where description like '%s%' limit 5;
select * from film where film_id is null;
select * from film where film_id is null limit 5;
select * from film order by description DESC;
select * from film order by description limit 5;
select * from film order by film_id limit 5;
select * from film order by film_id DESC;
select * from film order by title limit 5;
select * from film order by title DESC;
select * from film order by genre limit 5;
select * from film order by genre DESC;


